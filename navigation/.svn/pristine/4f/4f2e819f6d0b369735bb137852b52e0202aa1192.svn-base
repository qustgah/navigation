<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.navigation"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/test" >
        <activity
            android:name="com.navigation.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/concealTitle" ><!--控制页面显示，是否要标题，是否充满屏幕-->
            <!-- 点击应用图标，会进入该程序 -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="HeadActivity"
            android:logo="@drawable/title_icon"
          >
       </activity>
         <activity
            android:name="com.navigation.menufragment.Detail"
          >
       </activity>
        <activity
            android:name="com.navigation.SearchActivity"
          >
       </activity>
       <activity
            android:name="com.navigation.CategoryActivity"
          >
       </activity>
    </application>
</manifest>
